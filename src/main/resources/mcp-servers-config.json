{
  "mcpServers": {
    "qdrant-mcp-server": {
      "command": "MCP_SERVER_BOOT_COMMAND_PATH",
      "args": [
        "--transport",
        "stdio"
      ],
      "env": {
        "QDRANT_URL": "http://localhost:6333",
        "COLLECTION_NAME": "chatbot",
        "EMBEDDING_PROVIDER": "openai",
        "EMBEDDING_MODEL": "text-embedding-3-small",
        "OPENAI_API_KEY": "${spring.ai.openai.api-key}"
      }
    }
  }
}